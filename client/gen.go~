//+build generate

package main

import (
	"fmt"
	"log"
	"net/http"

	"github.com/shurcooL/vfsgen"
)

func main() {
	var fs http.FileSystem = http.Dir("./html/")
	fmt.Println("XX")

	err := vfsgen.Generate(fs, vfsgen.Options{
		PackageName:  "compiled",
		VariableName: "Assets",
	})
	if err != nil {
		log.Fatalln(err)
	}
}
